{
  "name": "typescript-rest-api",
  "version": "1.0.0",
  "description": "Typescript REST API  with Clean Architecture",
  "main": "index.ts",
  "scripts": {
    "start": "node ./dist/src/index.js",
    "dev": "npm run build && npm start",
    "test": "npx jest --detectOpenHandles --runInBand",
    "test:dev": "npx jest --detectOpenHandles --watchAll --runInBand",
    "build:ts": "npx tsc --build",
    "build:paths": "npx babel ./src --out-dir ./dist --extensions \".ts,.js\" --source-maps inline",
    "build": "npm run build:ts && npm run build:paths"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexandreSouzaRocha/typescript-rest-api.git"
  },
  "keywords": [
    "Typescript",
    "REST",
    "API",
    "Clean",
    "Architecture"
  ],
  "author": "Alexandre Rocha <alexandresouzarocha943@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexandreSouzaRocha/typescript-rest-api/issues"
  },
  "homepage": "https://github.com/AlexandreSouzaRocha/typescript-rest-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  }
}
